#!/usr/bin/env ruby
require_relative "../lib/turn.rb"

board = ["X", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"
display_board(board)

def move (num, character)
puts "Please ender 1-9:"
gets.(num, character)
(num - 1)
end


#board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

move(board, 4, "X")
